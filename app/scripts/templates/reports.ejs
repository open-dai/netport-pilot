<div class="row" id="reports">
  <div class="col-xs-6">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h4 class="panel-title">Nya rapporter</h4>
      </div>
      <div class="list-group">
        <% for(i=0; i<reports.length; i++) { %>
        <a href="#/reports/<%=reports[i].id%>" class="list-group-item">
          <h4 class="list-group-item-heading"><%=reports[i].title%></h4>
          <p class="list-group-item-text"><strong><%=reports[i].type%>:</strong> <%=reports[i].description%></p>
        </a>
        <% } %>
      </div>
    </div>
  </div>
</div>
