<div class="panel panel-default">
  <div class="panel-heading">
    <h3>Skapa ny rapport</h3>
  </div>
  <div class="panel-body">
    <div id="map"></div>
    <div class="form-group">
      <label for="types_id">Kategori</label>
      <select class="form-control" id="types_id">
      <% for(var i=0; i<types.length; i++) { %>
        <option value="<%=types[i].id%>"><%=types[i].title%></option>
      <% } %>
      </select>
    </div>
    <div class="form-group">
      <label for="description">Beskriv problemet</label>
      <textarea class="form-control" id="description" rows="7"></textarea>
    </div>
  </div>
  <!-- <div id="map"><span class="fa fa-spinner fa-spin"></span></div> -->
  <div class="panel-footer text-center">
  <button class="btn btn-primary" type="button">Skapa rapport</button>
  </div>
</div>
